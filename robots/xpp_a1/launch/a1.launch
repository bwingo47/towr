<launch>
  
  <!-- Upload URDF file to ros parameter server for rviz to find  -->
  <param name="a1_rviz_urdf_robot_description" command="$(find xacro)/xacro --inorder '$(find xpp_a1)/urdf/a1.urdf'"/>
  
  <!-- Converts Cartesian state to joint state and publish TFs to rviz  --> 
  <node name="urdf_visualizer_a1" pkg="xpp_a1" type="urdf_visualizer_a1" output="screen"/>
   
</launch>